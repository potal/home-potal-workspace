import threading

class MyThread(threading.Thread):
    '''
    classdocs
    '''


    def __init__(self,sock):
        '''
        Constructor
        '''
        self.sock = sock;
    def run(self):
        while True:
            buff = self.sock.recv(1024);
            print buff;